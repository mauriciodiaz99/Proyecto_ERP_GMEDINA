@model IEnumerable<ERP_GMEDINA.Models.tbSeleccionCandidatos>

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>SeleccionCandidatos</h2>
        <ol class="breadcrumb">
            <li class="active">
            </li>
        </ol>
    </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="float-e-margins">
                <div class="ibox-title">
                    <h5>Listado de SeleccionCandidatos</h5>
                    <div class="ibox-tools">
                        <input type="button" id="btnAgregar" class="btn btn-primary btn-xs" value="Registrar SeleccionCandidatos" />
                    </div>
                </div>
                <div class="ibox-content">
                  <table class="table table-striped table-bordered table-hover dataTables-example" id="IndexTable">
                    <thead>
                        <tr role="row">
                            <th>@Html.DisplayNameFor(model => model.scan_Fecha)</th>
                            <th>@Html.DisplayNameFor(model => model.tbFaseSeleccion.fsel_RazonInactivo)</th>
                            <th>@Html.DisplayNameFor(model => model.tbPersonas.per_Identidad)</th>
                            <th>@Html.DisplayNameFor(model => model.tbRequisiciones.req_Experiencia)</th>
                            <th>@Html.DisplayNameFor(model => model.tbFasesReclutamiento.fare_Descripcion)</th>
                            <th>Acciones</th>
                        </tr>
                    </thead>
                    <tbody>                    
                    </tbody>                       
                  </table>
                </div>
            </div>
        </div>
    </div>
 </div>
@Html.Partial("_Create", new ERP_GMEDINA.Models.tbSeleccionCandidatos())
@Html.Partial("_Delete", new ERP_GMEDINA.Models.tbSeleccionCandidatos())
@Html.Partial("_Edit", new ERP_GMEDINA.Models.tbSeleccionCandidatos())
@Html.Partial("_Detalles", new ERP_GMEDINA.Models.tbSeleccionCandidatos())
    
@section Styles {
    @Styles.Render("~/Content/plugins/dataTables/dataTablesStyles")
    @Styles.Render("~/Content/plugins/izitoast/iziToast")
}
@section Scripts {
@Scripts.Render("~/plugins/dataTables")
@Scripts.Render("~/Scripts/app/general")
@Scripts.Render("~/Scripts/app/general/SeleccionCandidatos")
}
